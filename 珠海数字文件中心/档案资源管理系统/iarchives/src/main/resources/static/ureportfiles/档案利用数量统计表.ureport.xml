  " select archivecode,title,filedate,type,doc.state, doc.borrowdate,count(archivecode) from tb_entry_index tei inner join tb_borrowmsg msg on tei.entryid = msg.entryid inner join tb_borrowdoc doc on doc.borrowcode = msg.borrowcode GROUP BY archivecode,title,filedate,doc.state,doc.borrowdate,type  HAVING doc.state=\'已通过\' "
   + ( emptyparam("lycopies")==false  ?  " and count(archivecode) in (\'" +replace(param("lycopies"),",","\',\'") + "\') " :  "")
   + ( emptyparam("jylx")==false && param("jylx")!='全部'  ?   " and type = \'" + param("jylx") + "\'" :  "")
   + ( emptyparam("starttime")==false && emptyparam("endtime")==false && param ("starttime") == param ("endtime")  ?  " and doc.borrowdate = \'" +  replace(param("starttime"),"-","") + "\'" : "" ) 
   + ( emptyparam("starttime")==false && emptyparam("endtime")==false && param ("starttime") != param ("endtime")  ?  " and doc.borrowdate >= \'" + replace(param("starttime"),"-","") + "\' and doc.borrowdate <= \'" + replace(param("endtime"),"-","") + "\' " : "" )
   + ( emptyparam("starttime")==false && emptyparam("endtime")==true  ?  " and doc.borrowdate >= \'" + replace(param("starttime"),"-","") + "\' " : "" )
   + ( emptyparam("starttime")==true && emptyparam("endtime")==false  ?  " and doc.borrowdate <= \'" + replace(param("endtime"),"-","") + "\' " :  "" )
}]]></sql><field name="archivecode"/><field name="title"/><field name="filedate"/><field name="type"/><field name="state"/><field name="borrowdate"/><field name="count(archivecode)"/><parameter name="lycopies" type="String" default-value=""/><parameter name="starttime" type="String" default-value=""/><parameter name="endtime" type="String" default-value=""/></dataset></datasource><paper type="A4" left-margin="28" right-margin="28"
